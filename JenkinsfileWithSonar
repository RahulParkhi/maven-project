Pipeline
{
agent any
stages
{
  stage ('SCM Checkout')
  {
    steps
    {
      
    }
  }
  stage ('Invoke maven goal with Sonarqube')
  {
    steps
    {
      withSonarQubeEnv(credentialsId: 'JenkinsSonarInt')
      {
        withMaven(jdk: 'Java', maven: 'Maven') {
          sh 'mvn clean sonar:sonar package'
      }
    }
   }
 }
}
      
